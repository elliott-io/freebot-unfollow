{
    "name": "FreeBot Unfollow: Instagram Mass Unfollower",
    "short_name": "FreeBot Unfollow",
    "version": "0.0.0.2", 
    "description": "Safe. Secure. Free. Unlimited. Automatic. Your personal mass unfollower bot for Instagram. ",
    "author": "elliott.io",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "content-scripts": [ {
        "matches": ["popup.html"],
        "js": ["progressbar.min.js"]
      }],
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "frame.html", "modal.html"
    ]
}
