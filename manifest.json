{
    "name": "FreeBot Unfollow: Unlimited Auto-Unfollower for Instagram",
    "version": "0.0.0.1", 
    "description": "Free, unlimited, automatic unfollower bot for Instagram. Powered by ads. You must be logged in to instagram.com in Chrome.",
    "author": "elliott.io",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "frame.html", "modal.html"
    ],
    "options_page": "options.html"
}
