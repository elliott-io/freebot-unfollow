{
    "name": "FreeBot Unfollow: Instagram Mass Unfollower",
    "short_name": "FreeBot Unfollow",
    "version": "0.0.0.2", 
    "description": "Safe. Secure. Free. Unlimited. Automatic. Your personal mass unfollower bot for Instagram. ",
    "author": "elliott.io",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "content-scripts": [ {
        "js": ["base.js"]
      }],
    "icons": {
        "16": "images/bot-Instagram-Glyph-Icon.rounded.16.png",
        "48": "images/bot-Instagram-Glyph-Icon.rounded.48.png",
        "128": "images/bot-Instagram-Glyph-Icon.rounded.128.png"
      },
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "frame.html", "modal.html", "background.html"
    ]
}
